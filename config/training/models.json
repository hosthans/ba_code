{
    "gan": {
        "name": "GAN",
        "epochs": 100,
        "lr": 2e-4, 
        "batch_size": 128,
        "shuffle": true,
        "momentum": 0.5,
        "unlabel_weight": 10,
        "n_critic": 5,
        "z_dim": 100,
        "img_size": 64 
