    "VGG16_celeba": {
        "name": "VGG16",
        "epochs": 200,
        "lr": 0.0005,
        "batch_size": 256,
        "shuffle": true,
        "momentum": 0.9,
        "weight_decay": 1e-4
    },

    "FaceNet":{
        "name": "FaceNet",
        "epochs":30,
        "batch_size":32,
